{
  "setupTasks": [
    "echo 'These tasks will run in order when initializing your CodeSandbox project'",
    "apt install node npm -y"
  ],
  "tasks": {
    "start": {
      "name": "start",
      "command": "gradio cc --help",
      "runAtStart": false
    },
    "build": {
      "name": "Build component",
      "command": "gradio cc build",
      "runAtStart": false
    },
    "dev": {
      "name": "Test component",
      "command": "gradio cc dev",
      "runAtStart": false
    },
  "install-dependencies": {
    "name": "Installing Dependencies",
    "command": "pip install gradio"
  }
}
